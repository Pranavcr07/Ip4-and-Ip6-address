import re
ip4= (25[0-4]|2[0-4][0-9]|[0-1][0-9][0-9]?)\.(25[0-4]|2[0-4][0-9]|[0-1][0-9][0-9]?)\.(25[0-4]|2[0-4][0-9]|[0-1][0-9][0-9]?)\.(25[0-4]|2[0-4][0-9]|[0-1][0-9][0-9]?)
def ipaddress(Ip):
   if re.search(ip4, Ip):
      print("Ip4")
   else:
      print("It is not correct ip address")
